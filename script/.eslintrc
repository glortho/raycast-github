{
  "root": true,
  "plugins": ["github"],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "extends": ["eslint:recommended", "prettier", "plugin:github/recommended"],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "github/no-then": "off",
    "import/no-commonjs": "off"
  }
}
